<Window x:Class="Diplom.Reg_Avto.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diplom.Reg_Avto"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Name" HorizontalAlignment="Left" Height="44" Margin="112,21,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="145"/>
        <TextBox x:Name="Surname" HorizontalAlignment="Left" Height="44" Margin="40,84,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="144"/>
        <TextBox x:Name="Nickname" HorizontalAlignment="Left" Height="44" Margin="211,139,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="145"/>
        <TextBox x:Name="Age" HorizontalAlignment="Left" Height="44" Margin="60,0,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Center" Width="145"/>
        
        
        <PasswordBox x:Name="Password" HorizontalAlignment="Left" Height="44" Margin="112,244,0,0"   VerticalAlignment="Top" Width="145"/>
        <PasswordBox x:Name="ConfirmPassword" HorizontalAlignment="Left" Height="44" Margin="112,293,0,0"   VerticalAlignment="Top" Width="145"/>

        <RadioButton x:Name="Male" Content="RadioButton" HorizontalAlignment="Left" Height="18" Margin="112,352,0,0" VerticalAlignment="Top" Width="109"/>
        <RadioButton x:Name="Female" Content="RadioButton" HorizontalAlignment="Left" Margin="226,352,0,0" VerticalAlignment="Top" Height="18" Width="91"/>

        <Button Content="Button" HorizontalAlignment="Left" Height="60" Margin="450,109,0,0" VerticalAlignment="Top" Width="184" Click="RegisterButton_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="60" Margin="606,8,0,0" VerticalAlignment="Top" Width="184" Click="BackButton_Click"/>
       

    </Grid>
</Window>
